{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/main-site/(.*)",
      "dest": "/main-site/$1"
    },
    {
      "src": "/booking-system/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/main-site/index.html"
    }
  ]
}